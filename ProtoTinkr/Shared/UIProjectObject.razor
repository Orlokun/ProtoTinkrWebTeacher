@using ProtoTinkr.Pages
<div class="bg-light border p-2 col-5">
    <h4 class="text"> @LProject.projectName</h4> <br/>
    <h4 class="text-secondary"> Horas Pedagógicas: @LProject.pedagogicalHours</h4>
    <input type="button" class="btn btn-danger" value="Borrar">
